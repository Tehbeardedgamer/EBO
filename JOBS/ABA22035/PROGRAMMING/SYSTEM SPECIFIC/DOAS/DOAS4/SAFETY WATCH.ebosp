NUMERIC INPUT CHW_DAT_A, HW_DAT_A, PH_DAT_A, CHW_DAT_B, HW_DAT_B, PH_DAT_B, SA_TEMP, LOW_TEMP_SP
NUMERIC OUTPUT SAFETY_SHUTDOWN

GOTO WATCHING

WATCHING:
IF ((CHW_DAT_A < LOW_TEMP_SP)OR (HW_DAT_A < LOW_TEMP_SP) OR (PH_DAT_A < LOW_TEMP_SP) OR (CHW_DAT_B < LOW_TEMP_SP) OR (HW_DAT_B < LOW_TEMP_SP) OR (PH_DAT_B < LOW_TEMP_SP) OR (SA_TEMP < LOW_TEMP_SP)) THEN 
SAFETY_SHUTDOWN = 1
Else
SAFETY_SHUTDOWN = 0
ENDIF
