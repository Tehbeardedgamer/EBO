NUMERIC DAY
NUMERIC OUTPUT WEEK_NUMBER


GOTO GET_WEEK

GET_WEEK:

DAY = DayOfMonth

IF (DAY >= 1 and DAY <= 7) THEN WEEK_NUMBER = 1
IF (DAY >= 8 and DAY <= 14) THEN WEEK_NUMBER = 2
IF (DAY >= 15 and DAY <= 21) THEN WEEK_NUMBER = 3
IF (DAY >= 22 and DAY <= 31) THEN WEEK_NUMBER = 4

GOTO WAIT_LOOP

WAIT_LOOP:
IF TS > 1800 THEN GOTO GET_WEEK