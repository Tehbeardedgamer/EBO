
Numeric Input SPACE_TEMP,SPACE_TEMP_SP
Numeric Output HW_CMD_CALC
Numeric OLDERR,PK,IK,DK,I_VALVE
String Input MODE
Function PID_CTRL

goto INITALIZE

INITALIZE:
OLDERR = 0
PK = 0.1
IK = 0.5
DK = 0
I_VALVE = 0

GOTO CONTROL_LOOP

CONTROL_LOOP:
If MODE ! "HEATING" THEN HW_CMD_CALC = 0

HW_CMD_CALC = PID_CTRL(0,SPACE_TEMP, SPACE_TEMP_SP, I_VALVE,PK,IK,DK,OLDERR)



