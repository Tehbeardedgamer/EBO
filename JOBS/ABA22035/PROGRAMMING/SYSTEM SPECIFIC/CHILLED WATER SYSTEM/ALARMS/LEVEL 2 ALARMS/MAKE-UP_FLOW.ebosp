Numeric INPUT CHW_MAKEUP_FLOW
Numeric OUTPUT CHW_MAKEUP_FLOW_ALARM,CHW_MAKEUP_CMD
Numeric TMR
Function SEC_TIMER

GOTO WAIT_LOOP

WATCHING:
If SEC_TIMER((CHW_MAKEUP_FLOW >= 3 ),120,TMR) then goto ALARM_ACTIVE
Else CHW_MAKEUP_FLOW_ALARM = 0



ALARM_ACTIVE:
WHILE CHW_MAKEUP_FLOW >=3  CHW_MAKEUP_FLOW_ALARM = 1
CHW_MAKEUP_CMD = 0
ENDWHILE
GOTO WAIT_LOOP

WAIT_LOOP:
if ts >10 Then goto WATCHING

