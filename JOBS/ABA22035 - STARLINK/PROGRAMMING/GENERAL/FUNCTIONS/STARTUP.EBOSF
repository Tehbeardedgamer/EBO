NUMERIC OUTPUT CH_ISO_CMD_1,CH_ISO_CMD_2,CH_ISO_CMD_3,CH_ISO_CMD_4,CH_ISO_CMD_5,CH_ISO_CMD_6,CH_ISO_CMD_7,CH_ISO_CMD_8,CH_ISO_CMD_9,CH_ISO_CMD_10,CH_ISO_CMD_11,CH_ISO_CMD_12
NUMERIC CH_ISO_CMD_[12], CH_FLOW[12], CH_ISO_STATUS[12], STARTUP, FLOW_MIN



ARG 1 i

CH_ISO_CMD_[i] = On 

    WHILE CH_ISO_STATUS[i] < 100 and CH_FLOW[i] < FLOW_MIN
    ENDWHILE

STARTUP = 1   

RETURN STARTUP
    
